{
  "name": "payload-starter",
  "version": "1.0.0",
  "description": "A blank template to get started with Payload 3.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "knip": "knip",
    "build": "cross-env NODE_OPTIONS=--no-deprecation payload migrate && next build",
    "postbuild": "next-sitemap --config next-sitemap.config.cjs",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev",
    "devsafe": "rm -rf .next && cross-env NODE_OPTIONS=--no-deprecation next dev",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start",
    "update:payload": "npm i @payloadcms/db-postgres@latest @payloadcms/storage-s3@latest @payloadcms/sdk@latest @payloadcms/next@latest @payloadcms/email-nodemailer@latest @payloadcms/ui payload@latest react@latest react-dom@latest @types/react@latest @types/react-dom@latest"
  },
  "dependencies": {
    "@fancyapps/ui": "^6.1.9",
    "@payloadcms/db-postgres": "^3.73.0",
    "@payloadcms/email-nodemailer": "^3.73.0",
    "@payloadcms/next": "^3.73.0",
    "@payloadcms/sdk": "^3.73.0",
    "@payloadcms/storage-s3": "^3.73.0",
    "@payloadcms/ui": "^3.73.0",
    "@pixi/react": "^8.0.5",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/themes": "^3.3.0",
    "@tanstack/react-query": "^5.90.19",
    "classnames": "^2.5.1",
    "cross-env": "^7.0.3",
    "embla-carousel-react": "^8.6.0",
    "graphql": "^16.10.0",
    "include-media": "^2.0.0",
    "lodash": "^4.17.21",
    "motion": "^12.23.26",
    "next": "^16.1.6",
    "next-sitemap": "^4.2.3",
    "nextjs-toploader": "^3.9.17",
    "payload": "^3.74.0",
    "payload-totp": "^3.0.0",
    "pixi.js": "^8.15.0",
    "qs-esm": "^7.0.3",
    "rc-pagination": "^5.1.0",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-error-boundary": "^6.1.0",
    "react-icons": "^5.5.0",
    "react-indiana-drag-scroll": "^2.2.1",
    "react-infinite-scroll-hook": "^6.0.1",
    "react-resizable-panels": "^3.0.6",
    "react-share": "^5.2.2",
    "react-use": "^17.6.0",
    "sharp": "^0.34.5",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@types/lodash": "^4.17.15",
    "@types/node": "^22.5.4",
    "@types/nodemailer": "^7.0.4",
    "@types/react": "^19.2.10",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.29.1",
    "@typescript-eslint/parser": "^8.50.0",
    "eslint": "^9.16.0",
    "eslint-config-next": "^15.5.9",
    "eslint-config-prettier": "^10.1.1",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-css-modules": "^2.12.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "knip": "^5.75.2",
    "prettier": "^3.7.4",
    "sass": "^1.97.0",
    "typescript": "^5.7.3"
  },
  "engines": {
    "node": "^18.20.2 || >=20.9.0"
  }
}
